{
  "name": "aidly",
  "version": "1.1.1",
  "description": "Tool library.",
  "main": "./dist/aidly.cjs.js",
  "unpkg": "./dist/aidly.umd.js",
  "module": "./dist/aidly.esm-bundler.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/aidly.esm-bundler.js",
      "require": "./dist/aidly.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "dev": "ts-node ./test.ts",
    "build": "rimraf ./dist && rollup --config",
    "format": "prettier --write --parser typescript \"(src|dist)/**/*.(js|ts?(x))\""
  },
  "author": "imtaotao",
  "keywords": [
    "utils",
    "shared",
    "outil",
    "aidly"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtaotao/aidly.git"
  },
  "bugs": {
    "url": "https://github.com/imtaotao/aidly/issues"
  },
  "packageManager": "pnpm@9.1.3",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.14.9",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.7",
    "rollup": "^2.67.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "small-queue": "^1.0.1"
  }
}
