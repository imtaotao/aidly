{
  "name": "aidly",
  "version": "1.22.0",
  "description": "Tool library.",
  "sideEffects": false,
  "main": "./dist/aidly.cjs.js",
  "unpkg": "./dist/aidly.umd.js",
  "module": "./dist/aidly.esm-bundler.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./dist/aidly.esm-bundler.mjs",
        "default": "./dist/aidly.esm-bundler.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/aidly.cjs.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "ts-node ./test.ts",
    "build": "rimraf ./dist && rollup --config && rimraf ./dist/__tests__",
    "build:watch": "rollup --config --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write --parser typescript \"(src|dist)/**/*.((m)?js|ts?(x))\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "imtaotao",
  "keywords": [
    "utils",
    "shared",
    "outil",
    "aidly"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imtaotao/aidly.git"
  },
  "bugs": {
    "url": "https://github.com/imtaotao/aidly/issues"
  },
  "packageManager": "pnpm@9.1.3",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "esbuild": "^0.23.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.5",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "small-queue": "^1.1.2"
  }
}
